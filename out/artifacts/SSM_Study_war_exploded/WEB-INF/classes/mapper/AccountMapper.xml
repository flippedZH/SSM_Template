<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="flipped.dao.AccountMapper">
    <select id="getAccountInfo" parameterType="String" resultType="flipped.pojo.Account">
        select * from `account` where `name`=#{name}
    </select>

</mapper>






